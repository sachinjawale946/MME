<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MME.Mobile.Views.PaymentPage"
             xmlns:toolkit="http://schemas.microsoft.com/dotnet/2022/maui/toolkit"
             Shell.BackgroundColor="{StaticResource AppPrimaryBackground}"
             xmlns:resource="clr-namespace:MME.Mobile.Resx"
             Title="{x:Static resource:AppResources.Payments}">

    <Grid VerticalOptions="CenterAndExpand" HorizontalOptions="CenterAndExpand">
        <Label Style="{StaticResource PageSubHeader}" HorizontalOptions="CenterAndExpand" VerticalOptions="Center" Text="{x:Static resource:AppResources.CommingSoon}"/>
    </Grid>
</ContentPage>